            <kind>1</kind>
            <string>///Movement And Attacking
{
    if(keyboard_check(vk_right) &amp;&amp; place_free(x+4,y) &amp;&amp; sprite_index != spr_player2_attack){
        x+=4;
        sprite_index = spr_player2_run;
        image_speed = .2;
        image_xscale = 1;
    }

    if(keyboard_check(vk_left) &amp;&amp; place_free(x-4,y) &amp;&amp; sprite_index != spr_player2_attack){
        x-=4;
        sprite_index = spr_player2_run;
        image_speed = .2;
        image_xscale = -1;
    }

    if(keyboard_check(vk_up) &amp;&amp; place_free(x,y-4) &amp;&amp; sprite_index != spr_player2_attack){
        y-=4;
        sprite_index = spr_player2_run;
        image_speed = .2;
    }
    
    if(keyboard_check(vk_down) &amp;&amp; place_free(x,y+4) &amp;&amp; sprite_index != spr_player2_attack){
        y+=4;
        sprite_index = spr_player2_run;
        image_speed = .2;
    }
    //Attacking
    if(keyboard_check_pressed(ord("A")) &amp;&amp; sprite_index != spr_player2_attack) {
        sprite_index = spr_player2_attack;
        image_speed = .5;
    }
    
    if(!keyboard_check(vk_right) &amp;&amp;  !keyboard_check(vk_left) &amp;&amp; !keyboard_check(vk_up) &amp;&amp; !keyboard_check(vk_down) &amp;&amp; sprite_index = !spr_player2_attack) {
    image_speed = 0;
    sprite_index = spr_player2_stand;
    }
    //Death
    if(global.hp &lt;= 0) game_end();
}
</string>
          </argument>
